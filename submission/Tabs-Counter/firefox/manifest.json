{
    "name": "Tab Counter",
    "version": "1.0.0",
    "description": "Counts the Tabs",
    "manifest_version": 2,
    "author": "Shourya Wadhwa",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Tab Counter",
        "default_icon": {
            "128":"icons/tab.png"
    }},
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    }
}